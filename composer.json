{
    "name":        "restcord/restcord",
    "description": "REST Library for the Discord API",
    "type":        "library",
    "require":     {
        "php":                        "^5.6|^7.0",
        "guzzlehttp/guzzle":          "^6.2",
        "guzzlehttp/guzzle-services": "^1.0",
        "symfony/options-resolver":   "^2.6|^3.2",
        "monolog/monolog":            "^1.22",
        "twig/twig":                  "^1.20|^2.0",
        "league/fractal":             "^0.15.0",
        "doctrine/annotations":       "^1.3",
        "doctrine/cache":             "^1.4",
        "doctrine/common":            "^2.3",
        "netresearch/jsonmapper":     "^1.1"
    },
    "autoload":    {
        "psr-4": {
            "RestCord\\": "src/"
        }
    },
    "license":     "MIT",
    "authors":     [
        {
            "name":  "Aaron Scherer",
            "email": "aequasi@gmail.com"
        }
    ],
    "require-dev": {
        "symfony/console":          "^2.6|^3.2",
        "beberlei/assert":          "^2.7",
        "gossi/php-code-generator": "^0.4.1",
        "symfony/var-dumper":       "^3.2"
    }
}
